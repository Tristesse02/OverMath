    <ui:VisualElement name="Container" class="background" style="flex-grow: 1; flex-direction: column; flex-wrap: nowrap; align-items: center; justify-content: flex-start; padding-top: 200px; background-image: url(&quot;project://database/Assets/UI/InstructionUI/Background.jpg?fileID=2800000&amp;guid=32c28f37421662640bfd0f18db72e00e&amp;type=3#Background&quot;); -unity-background-scale-mode: scale-and-crop; -unity-slice-top: 0; background-color: rgba(42, 42, 42, 0.82); -unity-background-image-tint-color: rgb(152, 152, 152);">
